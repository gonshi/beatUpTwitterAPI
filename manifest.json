{
  "name": "Twitter API叩き",
  "manifest_version": 2,
  "version": "2.1",
  "description": "Twitter APIを「本当に」叩くことを体験できます",
  "icons": { "48": "icon48.png",
          "128": "icon128.png" },
  "content_scripts":[
	  {
		  "matches": ["https://twitter.com/twitterapi", "https://twitter.com/twitterapi/"],
		  "js": ["/js/jquery.js", "/js/jquery.flash.js", "/js/transform2d.js", "/js/jquery.easing.js", "/js/main.js"],
		  "css": ["/css/main.css"]
	  }
  ],
  "web_accessible_resources": [
    "/img/crack.swf",
    "/img/crack_2.swf",
    "/img/bg.png",
    "/img/hammer2.png",
    "/img/hammer3.png",
  	"/img/exclamation.png",
    "/img/level1.png",
    "/img/level2.png",
    "/img/level3.png",
    "/img/level4.png",
    "/img/level5.png",
	"/img/twitter.gif",
    "/audio/final.mp3",
    "/audio/jab.mp3",
    "/audio/second.mp3",
    "/audio/swing.mp3",
    "/audio/bgm.mp3",
    "/audio/stamp.mp3",
    "/audio/striking.mp3",
    "/audio/gameover.mp3",
    "/audio/third.mp3"
  ]
}
